TopRule = ($* $D_JOT_INTENTS $*) {domain = 'jot'}{intent = D_JOT_INTENTS._intent};

#No prompt

D_JOT_INTENTS =
                $D_JOT_V_DISCARD {_intent='discard'} | 
                $D_JOT_V_CONTINUE {_intent='continue'} |
                $D_JOT_V_COMPLETE {_intent='complete'}
;

D_JOT_V_DISCARD = discard | delete | erase | cancel;
D_JOT_V_CONTINUE = continue | ( go on ) | ( move on );
D_JOT_V_COMPLETE = complete | finish;

