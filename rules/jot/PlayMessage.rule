TopRule = ($* $D_JOT_INTENTS $*) {domain='jot'}{intent = D_JOT_INTENTS._intent};

#No prompt. Jibo is showing on his screen an available, new message.

D_JOT_INTENTS = $D_JOT_PLAY_MESSAGES_INTENT{_intent = 'playMessage'};

D_JOT_PLAY_MESSAGES_INTENT = ($D_JOT_V_PLAY $* $D_JOT_V_MESSAGE);

D_JOT_V_MESSAGE = message|messages;
D_JOT_V_PLAY = query|show|display|(tell ?me)|(give ?me)|play;

